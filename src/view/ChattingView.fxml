<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="397.0" prefWidth="601.0" style="-fx-background-color: FFF9C4;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ChattingViewController">
   <children>
      <TextArea fx:id="chattf" editable="false" focusTraversable="false" layoutX="14.0" layoutY="14.0" prefHeight="331.0" prefWidth="406.0" wrapText="true" />
      <ListView fx:id="chtMember" focusTraversable="false" layoutX="428.0" layoutY="14.0" prefHeight="294.0" prefWidth="160.0" />
      <Button fx:id="btnInvite" focusTraversable="false" layoutX="428.0" layoutY="322.0" mnemonicParsing="false" onAction="#invite" prefHeight="23.0" prefWidth="70.0" text="초대" />
      <Button fx:id="btnExitChat" focusTraversable="false" layoutX="519.0" layoutY="322.0" mnemonicParsing="false" onAction="#exit" prefHeight="23.0" prefWidth="70.0" text="나가기" />
      <TextField fx:id="inputtf" layoutX="14.0" layoutY="360.0" onAction="#chatting" prefHeight="23.0" prefWidth="406.0" />
      <Button fx:id="sendMsg" focusTraversable="false" layoutX="428.0" layoutY="360.0" mnemonicParsing="false" onAction="#chatting" prefHeight="23.0" prefWidth="70.0" text="보내기" />
      <Button fx:id="sendFile" layoutX="519.0" layoutY="360.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="70.0" text="파일 전송" visible="false" />
   </children>
</AnchorPane>
